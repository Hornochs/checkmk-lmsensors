#!/usr/bin/python3
import sys
import sensors
def print_stuff():
    for chip in sensors.get_detected_chips():
        for feature in chip.get_features():
            print ("P {} count1={};65;75;0;100 Temperaturwert f√ºr {}".format(chip, chip.get_value(0), chip.get_label(feature)))    

def main():
    print_stuff()

if __name__ == '__main__':
    try:
        main()
    finally:
        sensors.cleanup()
